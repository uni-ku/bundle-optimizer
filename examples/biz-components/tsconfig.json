{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "preserve",
    "lib": ["esnext", "dom"],
    "useDefineForClassFields": true,
    "baseUrl": "./",
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["*"]
    },
    "resolveJsonModule": true,
    "types": ["@dcloudio/types"],
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true
  }
}
